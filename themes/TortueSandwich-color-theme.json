{
	"name": "TortueSandwich",
	"colors": {
		"editor.background": "#263238",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#479ed8",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#a0a0a0a4"
			}
		},
		{
			"scope": "meta.bracket.square.access",
			"settings": {
				"foreground": "#e4c1f9"
			}
		},
		{
			"scope": "variable.other.object",
			"settings": {
				"foreground": "#7ab7ab"
			}
		},
		{
			"scope": "templateType",
			"settings": {
				"foreground": "#FF0000"
			}
		},
		/*{
			"scope": "variable",
			"settings": {
				"foreground": "#a3b18a"
			}
		},*/
		
		
		//CPP
		{
			"scope": "storage.type.built-in.primitive.cpp",
			"settings": {
				"foreground": "#ffafcc"
			}
		},
		{
			"scope": "entity.name.type.cpp",
			"settings": {
				"foreground": "#fb6aa9"
			}
		},
		{	
			"name": "Struct",
			"scope": "entity.name.type.struct.cpp",
			"settings": {
				"foreground": "#f77f00"
			}
		},
		{
			"name": "chiffres",
			"scope": "constant.numeric.decimal.cpp",
			"settings": {
				"foreground": "#e4c1f9"
			}
		},
		{
			"name": "Couleur string char",
			"scope": ["string.quoted.single.cpp","string.quoted.double.cpp"],
			"settings": {
				"foreground": "#80ffdb"
			}
		},
		{
			"scope": "keyword.control.directive.include.cpp",
			"settings": {
				"foreground": "#824343"
			}
		},
		{
			"scope": ["string.quoted.other.lt-gt.include.cpp","string.quoted.double.include.cpp"],
			"settings": {
				"foreground": "#c47878"
			}
		},

		
		
		
		//PYTHON
		{	
			"name": "Constantes",
			"scope": "constant.other.caps.python",
			"settings": {
				"foreground": "#1f80a1"
			}
		},
		{
			"name": "Strings",
			"scope": "string.quoted.single.python",
			"settings": {
				"foreground": "#FF0000"
			}
		},
	
		
	]
}
